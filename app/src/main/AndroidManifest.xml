<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.digits.business">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>

    <application
        android:name="com.digits.business.app.MyApplication"
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:usesCleartextTraffic="true"
        android:icon="@mipmap/ic_launcher2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <!-- [START fcm_listener] -->
        <service android:name=".twilio.fcm.VoiceFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <!-- [END fcm_listener] -->
        <!-- [START instanceId_listener] -->
        <!--<service android:name=".twilio.fcm.VoiceFirebaseInstanceIDService">-->
            <!--<intent-filter>-->
                <!--<action android:name="com.google.firebase.INSTANCE_ID_EVENT" />-->
            <!--</intent-filter>-->
        <!--</service>-->

        <service
            android:name=".twilio.Voice"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".twilio.vc"/>
        <activity
            android:name=".twilio.LoginActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name=".twilio.RegisterActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".twilio.DashboardActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".twilio.VoiceActivity"
            android:label="@string/make_call"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            android:parentActivityName="com.digits.business.activities.MainActivity"
            android:screenOrientation="portrait"/>


        <service android:name="com.digits.business.fcm.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity android:name="com.digits.business.activities.AboutUsActivity"
            android:label="@string/about_us"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.digits.business.twilio_old.VoiceActivityOld"
            android:parentActivityName="com.digits.business.activities.MainActivity"
            android:screenOrientation="portrait"/>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3940256099942544~3347511713" />
        <activity
            android:name="com.digits.business.activities.ImagesViewerBusinessActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.digits.business.activities.SaveTTSActivity"
            android:label="@string/title_activity_save_tts"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.digits.business.activities.SearchForBusinessActivity"
            android:label="@string/title_activity_search_business"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.digits.business.activities.BusinessDetailsActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/title_activity_business_details"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.digits.business.activities.SearchMp3FilesActivity"
            android:label="@string/title_activity_search_Mp3_files"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.digits.business.activities.SettingActivity"
            android:label="@string/title_activity_setting"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity android:name="com.digits.business.activities.PlayMp3FilesActivity"
            android:label="@string/play_voice"
            android:screenOrientation="portrait"/>
        <activity android:name="com.digits.business.activities.SaveBusinessActivity"
            android:label="@string/add_new_business"
            android:screenOrientation="portrait"/>
        <activity android:name="com.digits.business.activities.UploadeMp3FileActivity"
            android:label="@string/title_activity_mp3_files2"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.digits.business.activities.VoiceMailActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/title_activity_mp3_files"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.digits.business.activities.UploadGreetingActivity2"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/title_activity_mp3_files2"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.digits.business.activities.ImagesViewerActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity android:name="com.digits.business.activities.EditMyProfileActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.digits.business.activities.MainActivity"
            android:label="@string/headline_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.digits.business.activities.RegisterActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name="com.digits.business.activities.LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.digits.business.phoneauth.PhoneNumberAuthActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name="com.digits.business.phoneauth.VerifyPhoneActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity android:name="com.digits.business.activities.ProfileActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.digits.business.classes.GalleryUtil"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="${applicationId}.provider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/provider_paths" />
        </provider>
    </application>

</manifest>